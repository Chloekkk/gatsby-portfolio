{
  "name": "gatsby-typescript-boilerplate",
  "description": "A starter kit for TypeScript-based Gatsby projects with sensible defaults.",
  "version": "1.0.0",
  "private": true,
  "author": "Chloekkk <skdusdl8804@gmail.com>",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/mdx": "v1",
    "@mdx-js/react": "v1",
    "babel-plugin-styled-components": "^2.0.2",
    "classnames": "^2.3.1",
    "gatsby": "^4.7.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-canonical-urls": "^4.7.0",
    "gatsby-plugin-emotion": "^7.7.0",
    "gatsby-plugin-mdx": "^3.7.1",
    "gatsby-plugin-react-helmet": "^5.7.0",
    "gatsby-plugin-sharp": "^4.7.0",
    "gatsby-plugin-typescript": "^4.7.0",
    "gatsby-source-filesystem": "^4.7.0",
    "gatsby-transformer-json": "^4.7.0",
    "gatsby-transformer-sharp": "^4.7.0",
    "modern-normalize": "^1.1.0",
    "normalize.css": "^8.0.1",
    "polished": "^4.1.4",
    "prism-themes": "^1.9.0",
    "prismjs": "^1.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.7.0",
    "@types/classnames": "^2.3.0",
    "@types/node": "^17.0.18",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/unist": "^2.0.6",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-blvd": "^1.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  }
}
